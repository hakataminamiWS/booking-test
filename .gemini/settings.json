{
    "mcpServers": {
        "laravel": {
            "command": "php",
            "args": ["./artisan", "boost:mcp"],
            "timeout": 30000,
            "trust": false
        },
        "vuetify": {
            "command": "npx",
            "args": ["-y", "@vuetify/mcp"]
        }
    }
}
